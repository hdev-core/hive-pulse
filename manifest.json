
{
  "manifest_version": 3,
  "name": "Hive Frontend Switcher",
  "version": "1.1.0",
  "description": "Switch between Hive frontends, share links, and launch DApps.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Hive Switcher",
    "default_icon": "icon.svg"
  },
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "alarms"
  ]
}
